    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "node": "^22.9.0",
    "yargs": "^17.7.2"
  },
  "bin": {
    "fockbuilder": "./bin/index.js"
  },
