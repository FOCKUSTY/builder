{
  "name": "fock-builder",
  "version": "0.2.1",
  "description": "simple builder for you library or project",
  "main": "index.ts",
  "scripts": {
    "dev:config": "set NODE_ENV=config&& nodemon ./start.ts",
    "build:tsc": "tsc",
    "build:builder": "set NODE_ENV=build&& ts-node ./start.ts",
    "build": "npm run build:tsc && npm run build:builder"
  },
  "keywords": [
    "libs",
    "builder",
    "copier"
  ],
  "devDependencies": {
    "@types/node": "^22.7.4",
